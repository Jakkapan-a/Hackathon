
# ================ OCR Configuration ================
# OCR Method: 'ollama' or 'api'
OCR_METHOD=api
# ================ Path Configuration ================
# Base directory for input files (contains Test_doc_info.csv, Test_nacc_detail.csv, etc.)
INPUT_BASE_DIR="./test_final/test final input"

INPUT_CSV_DOC_INFO="Test final_doc_info.csv"
INPUT_CSV_NACC_DETAIL="Test final_nacc_detail.csv"
INPUT_CSV_SUBMITTER="Test final_submitter_info.csv"
OUTPUT_PREFIX="Test final"

# PDF folder path (relative to INPUT_BASE_DIR)
PDF_FOLDER="Test final_pdf"

# Output directory for JSON and CSV results
OUTPUT_DIR="./test_final/test_final_output"


# Ollama Configuration OCR (for extract_text_from_image_ollama)
OLLAMA_BASE_URL=http://localhost:7869/v1
OLLAMA_MODEL=scb10x/typhoon-ocr1.5-3b:latest

# OCR Settings
OCR_TEMPERATURE=0.1
OCR_TOP_P=0.6

# ================ OpenTyphoon API Configuration ================
# OpenTyphoon API Configuration (for extract_text_from_image)
TYPHOON_API_KEY=your_api_key_here
TYPHOON_MODEL=typhoon-ocr
TYPHOON_TASK_TYPE=default
TYPHOON_MAX_TOKENS=16000
TYPHOON_TEMPERATURE=0.1
TYPHOON_TOP_P=0.6
TYPHOON_REPETITION_PENALTY=1.1

# ================ LLM Parser Configuration (Phase 3) ================
# OpenAI API Key for GPT-4.1-mini
OPENAI_API_KEY=your_openai_api_key_here

# LLM Model for parsing (default: gpt-4.1-mini)
LLM_MODEL=gpt-4.1-mini

# LLM Parse Mode: "combined" (all pages at once) or "page_by_page" (parse each page then merge)
LLM_PARSE_MODE=page_by_page

# Number of parallel workers for page-by-page mode
LLM_MAX_WORKERS=5

# Number of documents to process in parallel (Multi-Task)
# Set to 1 for sequential processing, 2+ for parallel processing
LLM_PARALLEL_DOCS=1

# ================ Phase Skip Configuration ================
# Set to "true" to skip each phase (for debugging/testing)
SKIP_PHASE_1=false
SKIP_PHASE_2=false
SKIP_PHASE_3=false
SKIP_PHASE_4=false
SKIP_PHASE_5=false

